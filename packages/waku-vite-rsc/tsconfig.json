{
  "compilerOptions": {
    "noEmit": true,
    "lib": ["esnext", "dom", "dom.iterable"],
    "strict": true,
    "target": "esnext",
    "downlevelIteration": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "bundler",
    "skipLibCheck": true,
    "verbatimModuleSyntax": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noUncheckedIndexedAccess": true,
    "jsx": "react-jsx"
  }
}
